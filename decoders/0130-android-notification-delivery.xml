<decoder name="android_notification_delivery">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: Notification Delelivery Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation:\s+Notification Delelivery Detected\s+(?i)Uid\s+(\d+)\s+Listner\s+(\S+)</regex>
  <order>deviceId,apkHash,uid,component</order>
</decoder>
