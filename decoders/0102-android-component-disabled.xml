<decoder name="android_component_disabled">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: Component Disabled Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation: Component Disabled Detected\s+(?i)uid\s+(\d+)\s+Name\s+(\S+)\s+State\s+(\d+)</regex>
  <order>deviceId,apkHash,uid,component_name,state</order>
</decoder>
