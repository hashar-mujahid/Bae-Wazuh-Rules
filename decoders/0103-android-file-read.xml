<decoder name="android_file_read">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <fts>location</fts>
  <prematch>Operation: File Read Operation Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation: File Read Operation Detected\s+uid\s+(\d+)\s+Path\s+(\S+)</regex>
  <order>deviceId,apkHash,uid,path</order>
</decoder>
