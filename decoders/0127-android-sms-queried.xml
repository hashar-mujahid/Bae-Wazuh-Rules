<!-- Modify it at your will. -->
<decoder name="android_sms_query">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: SMS queried Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation:\s+SMS queried Detected by\s+(?i)Uid\s+(\d+),\s+Package:\s+(\S+),\s+Uri:\s+(\S+)</regex>
  <order>deviceId,apkHash,uid,package,uri</order>
</decoder>
