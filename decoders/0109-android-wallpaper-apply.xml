<decoder name="android_wallpaper_apply">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: Wallpaper Apply Operation Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation: Wallpaper Apply Operation Detected\s+(?i)uid\s+(\d+)\s+Name\s+(\S+)</regex>
  <order>deviceId,apkHash,uid,name</order>
</decoder>
