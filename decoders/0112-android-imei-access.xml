<decoder name="android_imei_access">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: IMEI Accessed Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation: IMEI Accessed Detected\s+(?i)uid\s+(\d+)</regex>
  <order>deviceId,apkHash,uid</order>
</decoder>
