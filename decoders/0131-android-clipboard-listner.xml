<decoder name="android_clipboard_listener_registered">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: Clipboard Listner Registered Detected</prematch>
  <regex type="pcre2">
    Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operaton:\s+Clipboard Listner Registered Detected\s+(?i)Uid\s+(\d+)\s+Package\s+(\S+)
  </regex>
  <order>deviceId,apkHash,uid,package</order>
</decoder>
