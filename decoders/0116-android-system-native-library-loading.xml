<decoder name="android_native_library_loading">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: Native Library Loading Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation: Native Library Loading Detected\s+(?i)Uid\s+(\d+)\s+Filename\s+(\S+)</regex>
  <order>deviceId,apkHash,uid,filename</order>
</decoder>
