<decoder name="android_autonomous_gestures">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: Autonomous Gestures Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation: Autonomous Gestures Detected\s+(?i)uid\s+(\d+)\s+action\s+(\d+)</regex>
  <order>deviceId,apkHash,uid,action</order>
</decoder>
