<decoder name="android_network_activity">
  <parent>android_log_base</parent>
  <use_own_name>true</use_own_name>
  <prematch>Operation: URL Detected</prematch>
  <regex type="pcre2">Device:\s+(\S+)\s+Apk_Hash:\s+(\S+)\s+Operation:\s+URL\s+Detected\s+uid\s+(\d+)\s+URL\s+(https?:\/\/\S+)</regex>
  <order>deviceId,apkHash,uid,url</order>
</decoder>
